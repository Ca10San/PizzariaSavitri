        "readonly": "true",
        "values": [{
            "__query": {
                "module": "customer",
                "index": [ "name" ]
            }
        }]
    },
    "orderID": {
        "label": "Numero do pedido",
        "type": "number",
        "values": [{
            "__query": {
                "module": "order",
                "index": [ "orderID" ]
            }
        }]
    }
    },
    "filters": [
    "customer",
    "orderID"
    ]
}